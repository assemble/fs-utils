{
  "name": "fs-utils",
  "description": "fs extras and utilities to extend the node.js file system module. Used in Assemble and many other projects.",
  "version": "0.7.0",
  "homepage": "https://github.com/jonschlinkert/fs-utils",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/fs-utils",
  "bugs": {
    "url": "https://github.com/jonschlinkert/fs-utils/issues"
  },
  "license": "MIT",
  "files": [
    "index.js",
    "utils.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">= 0.8.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "async": "^1.5.0",
    "delete": "^0.3.2",
    "extend-shallow": "^2.0.1",
    "graceful-fs": "^4.1.5",
    "is-absolute": "^0.2.5",
    "js-yaml": "^3.6.1",
    "kind-of": "^3.0.4",
    "lazy-cache": "^2.0.1",
    "matched": "^0.4.3",
    "normalize-path": "^2.0.1",
    "read-data": "^0.3.0",
    "read-yaml": "^1.0.0",
    "relative": "^3.0.2",
    "write": "^0.3.1",
    "write-data": "^0.1.0",
    "write-json": "^0.2.2",
    "write-yaml": "^0.2.2"
  },
  "devDependencies": {
    "gulp-format-md": "^0.1.10",
    "mocha": "^3.0.1",
    "should": "^10.0.0"
  },
  "keywords": [
    "file",
    "file system",
    "fs",
    "node",
    "node.js",
    "path",
    "read",
    "readFile",
    "readFileSync",
    "utils"
  ],
  "verb": {
    "related": {
      "list": [
        "read-data",
        "read-yaml",
        "write-data",
        "write-json",
        "write-yaml"
      ]
    },
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "lint": {
      "reflinks": true
    },
    "reflinks": [
      "verb",
      "verb-generate-readme"
    ]
  }
}